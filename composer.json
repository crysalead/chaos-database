{
    "name": "crysalead/chaos-database",
    "description": "PDO driver for Chaos.",
    "keywords": ["PDO", "Database driver"],
    "license": "MIT",
    "require": {
        "php": ">=8.3",
        "crysalead/set": "^2",
        "crysalead/chaos-orm": "dev-master",
        "crysalead/sql-dialect": "dev-master"
    },
    "require-dev": {
        "kahlan/kahlan": "^6"
    },
    "autoload": {
        "psr-4": {
            "Chaos\\Database\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chaos\\Database\\Spec\\": "spec/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev"
}
